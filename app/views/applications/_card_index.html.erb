<div class="card mx-auto">
    <h5 class="card-header"><%= application.job.title %> in <%= application.job.location.city %></h5>
    <div class="card-body">
        <p class="card-text"><%= application.job.description %></p>
        <%= button_to "View Application", job_application_path(application.job, application), class: "btn btn-outline-primary float-end", method: :get %>
    </div>
</div>
<br>